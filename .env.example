# Convex deployment
CONVEX_DEPLOYMENT=

# Auth providers (Convex Auth)
AUTH_ANONYMOUS_ENABLED=true
AUTH_EMAIL_ENABLED=false
AUTH_GOOGLE_ENABLED=false
AUTH_GITHUB_ENABLED=false

# AI Provider API Keys (Server-side)
# These are used when users don't provide their own keys
CONVEX_OPENAI_API_KEY=
CONVEX_OPENAI_BASE_URL=https://api.openai.com/v1
CONVEX_ANTHROPIC_API_KEY=
CONVEX_GOOGLE_API_KEY=

# Search API
CONVEX_BRAVE_SEARCH_API_KEY=

# Optional: Default OpenRouter key for demo purposes
CONVEX_OPENROUTER_API_KEY=
